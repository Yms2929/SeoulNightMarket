<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seoulnightmarket.seoulnightmarket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InitialScreen" />
        <activity android:name=".NightMarketFragment" />
        <activity android:name=".FragmentIntroduction" />
        <activity android:name=".FragmentMarket" />
        <activity android:name=".FragmentConcert" />
        <activity android:name=".FragmentReview" />
        <activity android:name=".FoodTruckFragment" />
        <activity android:name=".FragmentMenu" />
        <activity android:name=".FragmentInformation"></activity>
    </application>

</manifest>